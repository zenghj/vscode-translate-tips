{
	"name": "translate-tips",
	"displayName": "translate-tips",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:vue"
	],
	"main": "./out/extension.js",
	"contributes": {
		"capabilities": {
			"hoverProvider": "true"
		},
		"configuration": {
			"title": "translate-tips",
			"properties": {
				"translate-tips.localeFilePath": {
					"type": "string",
					"default": null,
					"description": "absolute path of locale file, support .js、.json、.jsx, .ts、.tsx file"
				},
				"translate-tips.keyReg": {
					"type": "string",
					"description": "pattern to match words for those need show tips",
					"default": "*"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"publish": "vsce publish",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zenghj/vscode-translate-tips.git"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"chokidar": "^3.4.0"
	},
	"publisher": "Julianzeng"
}
